<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape
            android:shape="oval">

            <solid
                android:color="#44f52323"/>

        </shape>
    </item>
    <item android:state_enabled="true">
        <shape
            android:shape="oval">

            <solid
                android:color="#f52323"/>

                <stroke android:color="#3d9f9f9f"
                    android:width="2dp"/>
        </shape>
    </item>
</selector>
